<?xml version="1.0" encoding="UTF-8"?>
<wabacus-config xmlns="http://www.wabacus.com"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.wabacus.com xsd/wabacus.cfg.xsd">
	<system>
		<item name="webroot" value="/HEUConStu" /><!-- 应用的根URL -->
		<item name="webroot-abspath" value="" />
		<!--
			默认为context.getRealPath("/")获取的值，可以直接配置根路径
		-->
		<item name="showreport-url" value="/ShowReport.wx" />
		<!--
			显示报表数据的URL，就是在web.xml中配置的<url-pattern/>的值可以带参数
		-->
		<item name="encode" value="UTF-8" />
		<item name="show-sql" value="true" /><!-- 是否显示执行的SQL语句或存储过程 -->
		<item name="default-cellresize" value="2" />
		<!--
			指定整个项目中所有单行表头的数据自动列表报表的表格单元格默认可移动性
		-->
		<item name="default-condition-labelposition" value="inner" />
		<!--
			查询条件提示标签的显示位置
		-->
		<item name="server-validate-class" value="" />
		<!--
			自己开发的服务器端校验类
		-->
		<item name="skin" value="qq" /><!-- 皮肤样式 -->
		<item name="default-checkpermission" value="true" />
		<!--
			本应用是否要检查权限，如果在不需要检查权限的应用中关闭它，可以改善性能
		-->
		<item name="prompt-dialog-type" value="ymprompt" />
		<!--
			提示组件类型 ymprompt;artdialog
		-->
	</system>

	<datasources default="ds_mysql">

		<datasource name="ds_mysql"
			type="com.wabacus.config.database.datasource.C3P0DataSource" dbtype="com.wabacus.config.database.type.MySql">
			<property name="driver">com.mysql.jdbc.Driver</property>
			<property name="url"><![CDATA[jdbc:mysql://localhost/edusys?useUnicode=true&zeroDateTimeBehavior=convertToNull&characterEncoding=UTF-8]]></property>

			<property name="user">root</property>

			<property name="password">123456</property>
			<property name="max_size">80</property>
			<property name="min_size">5</property>
			<property name="timeout">100</property>
			<property name="max_statements">100</property>
			<property name="idle_test_period">50</property>
			<property name="acquire_increment">2</property>
		</datasource>
		<!-- 演示Proxool配置的数据源 -->
		<!--
			datasource name="ds_mysql_proxool"
			type="com.wabacus.config.database.datasource.ProxoolDataSource"
			dbtype="com.wabacus.config.database.type.MySql"> <property
			name="alias">proxool_pool</property> <property
			name="configfile">proxool.xml</property> </datasource
		-->
		<!-- 演示JNDI方式配置的数据源 -->
		<!--
			datasource name="ds_sqlserver2k5"
			type="com.wabacus.config.database.datasource.JNDIDataSource"
			dbtype="com.wabacus.config.database.type.SQLSERVER2K5"> <property
			name="jndi">java:comp/env/demoSource</property> </datasource
		-->
		<!-- 演示直接用DriverManager配置的数据源 -->
		<!--
			datasource name="ds_sqlserver2k5"
			type="com.wabacus.config.database.datasource.DriverManagerDataSource"
			dbtype="com.wabacus.config.database.type.SQLSERVER2K5"> <property
			name="driver">net.sourceforge.jtds.jdbc.Driver</property> <property
			name="url">jdbc:jtds:sqlserver://localhost:1433;DatabaseName=szseWebDB3</property>
			<property name="user">sa</property> <property
			name="password">111111</property> </datasource
		-->
	</datasources>

